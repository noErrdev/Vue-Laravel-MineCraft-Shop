{
    "name": "d3lph1/l-shop",
    "description": "Modern e-commerce system for Minecraft.",
    "keywords": ["shop", "e-shop", "e-commerce", "trading", "bukkit", "minecraft", "l-shop", "laravel"],
    "authors": [
        {
            "name": "Chertkov Bogdan",
            "email": "d3lph1.contact@gmail.com",
            "homepage": "https://github.com/D3lph1",
            "role": "Backend/Frontend developing"
        },
        {
            "name": "Vlasov Michael",
            "homepage": "https://github.com/Whiled0S",
            "role": "Design, layout, frontend developing"
        }
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.0",
        "beberlei/DoctrineExtensions": "~1.0",
        "d3lph1/minecraft-rcon-manager": "~1.2",
        "fideloper/proxy": "~3.3",
        "intervention/image": "~2.4",
        "laravel-doctrine/migrations": "~1.1",
        "laravel-doctrine/orm": "~1.4",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0"
    },
    "require-dev": {
        "filp/whoops": "~2.1",
        "fzaninotto/faker": "~1.7",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.5",
        "symfony/thanks": "~1.0",
        "barryvdh/laravel-ide-helper": "~2.4"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
